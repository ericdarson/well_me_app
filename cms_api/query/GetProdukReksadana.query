select 
    p.id_produk,
	p.nama_produk,
	p.nab,
	p.minimum_unit,
	p.expense_ratio,
	p.total_aum,
	p.manager_investasi,
	p.tingkat_resiko,
	p.level_resiko,
	p.bank_kustodian,
	p.bank_penampung,
	p.weekly_nab,
	p.monthly_nab,
	p.three_months_nab,
	p.year_on_year_nab,
	p.id_jenis_reksadana,
	j.nama_jenis,
	URL_VENDOR,
	PASSWORD_VENDOR
from T_PRODUK_REKSADANA P JOIN M_JENIS_REKSADANA j ON p.id_jenis_reksadana = j.id_jenis_reksadana
where j.id_jenis_reksadana %s (%s)